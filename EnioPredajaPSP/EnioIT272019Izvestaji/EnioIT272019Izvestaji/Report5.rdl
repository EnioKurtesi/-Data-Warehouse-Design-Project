  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT       Hockey.Official.official_fullname, SUM(Hockey.Fact_table.total_hits) AS total
FROM            Hockey.Fact_table INNER JOIN
                         Hockey.Official ON Hockey.Fact_table.official_id = Hockey.Official.official_id INNER JOIN
                         Hockey.Date_dimension ON Hockey.Fact_table.date_id = Hockey.Date_dimension.DateKey
WHERE        (YEAR(Hockey.Date_dimension.Date) BETWEEN @start AND @end) OR
                         (@start IS NULL) AND (@end IS NULL)
GROUP BY Hockey.Official.official_fullname
ORDER BY total DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="official_fullname">
          <DataField>official_fullname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="total">
          <DataField>total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!total.Value</FilterExpression>
          <Operator>TopN</Operator>
          <FilterValues>
            <FilterValue>=Parameters!top.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
